enum UserType {
  ADMIN
  STAFF
  STUDENT
}

type User {
  id: ID! @id
  isActive: Boolean! @default(value: true)
  userType: UserType!
  email: String! @unique
  password: String!
  firstName: String
  lastName: String
  address: String
  opNro: String
  puh: String

}

type Device {
  id: ID! @id
  idCode: String
  model: String
  info: String
  devCategoryId: DevCategory! 

}

type DevCategory {
  id: ID! @id
  deviceType: String
  manufacture: String

}

type Loan {
  id: ID! @id
  loanDate: String!
  returnDate: String
  dueDate: String!
  deviceId: Device!
  loanerId: User! @relation(name: "LoanerId")
  supplierId: User! @relation(name: "SupplierId")
  returnerId: User @relation(name: "ReturnId")
}
